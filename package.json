{
  "name": "artipub",
  "version": "0.1.4",
  "description": "Article publishing platform that automatically distributes your articles to various media channels",
  "main": "./app/main.js",
  "scripts": {
    "start": "node cli.js start",
    "start:frontend": "umi dev",
    "start:backend": "nodemon server.js",
    "build": "umi build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "electron-dev": "nodemon --watch ./app/main.js --exec 'NODE_ENV=development electron .'",
    "app:build": "electron-webpack build",
    "dist": "npm run build && npm run app:build && electron-builder"
  },
  "homepage": "https://github.com/crawlab-team/artipub#readme",
  "build": {
    "asar": false,
    "appId": "artipub",
    "productName": "artipub",
    "copyright": "Copyright Â© 2020 mtianyan",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "models/",
      "routes/",
      "lib/",
      "settings/**/*",
      "package.json",
      "server.js",
      "config.js",
      "constants.js",
      "data.js",
      "exec.js",
      "init.js",
      "logger.js",
      "spiders/"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    },
    "extraMetadata": {
      "main": "./dist/app/main.js"
    },
    "publish": [
      "github"
    ],
    "extends": null,
    "mac": {
      "category": "public.app-category.productivity",
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "dmg": {
      "background": "assets/appdmg.png",
      "icon": "assets/icon.icns",
      "iconSize": 100,
      "contents": [
        {
          "x": 380,
          "y": 280,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 110,
          "y": 280,
          "type": "file"
        }
      ],
      "window": {
        "width": 500,
        "height": 500
      }
    },
    "win": {
      "target": [
        "msi",
        "nsis"
      ],
      "icon": "assets/icon.ico",
      "artifactName": "${productName}-Web-Setup-${version}.${ext}",
      "publisherName": "mtianyan"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": false
    }
  },
  "bin": {
    "artipub": "./cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crawlab-team/artipub.git"
  },
  "keywords": [
    "nodejs",
    "publish",
    "article",
    "media",
    "write",
    "post",
    "puppeteer"
  ],
  "author": "Marvin Zhang",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/crawlab-team/artipub/issues"
  },
  "dependencies": {
    "async-lock": "^1.2.2",
    "axios": "^0.20.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "clipboardy": "^2.1.0",
    "conf": "^7.1.2",
    "cors": "^2.8.5",
    "cron": "^1.7.1",
    "electron-localshortcut": "^3.2.1",
    "express": "^4.17.1",
    "level-js": "^4.0.2",
    "leveldown": "^5.6.0",
    "linvodb3": "^3.26.0",
    "log4js": "^5.1.0",
    "morgan": "^1.9.1",
    "portfinder": "1.0.28",
    "puppeteer-chromium-resolver": "^2.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "showdown": "^1.9.0",
    "source-map-support": "0.5.16"
  },
  "devDependencies": {
    "@ant-design/colors": "^3.1.0",
    "@ant-design/pro-cli": "^1.0.0",
    "@ant-design/pro-layout": "^4.5.9",
    "@antv/data-set": "^0.10.2",
    "@types/classnames": "^2.2.7",
    "@types/codemirror": "0.0.76",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.133",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.19",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.8.4",
    "@types/showdown": "^1.9.3",
    "@umijs/fabric": "^1.1.0",
    "antd": "^3.20.0",
    "async-lock": "^1.2.2",
    "caporal": "^1.3.0",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "classnames": "^2.2.6",
    "codemirror": "^5.48.2",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.1.1",
    "dva": "^2.4.1",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^6.0.0",
    "electron-webpack": "^2.8.2",
    "enzyme": "^3.9.0",
    "eslint": "^5.16.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "github-markdown-css": "^3.0.1",
    "hash.js": "^1.1.7",
    "husky": "^3.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest-puppeteer": "^4.2.0",
    "level-js": "^4.0.2",
    "leveldown": "^5.6.0",
    "lint-staged": "^9.0.0",
    "linvodb3": "^3.26.0",
    "lodash": "^4.17.11",
    "markdown-it": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.24.0",
    "netlify-lambda": "^1.4.13",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.4",
    "numeral": "^2.0.6",
    "nzh": "^1.0.4",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.0.0",
    "prettier": "^1.17.1",
    "pro-download": "1.0.1",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-ace": "^7.0.2",
    "react-codemirror2": "^6.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-markdown-editor-lite": "^0.4.3",
    "react-showdown": "^1.6.0",
    "redux": "^4.0.1",
    "request": "^2.88.0",
    "serverless-http": "^2.0.2",
    "slash2": "^2.0.0",
    "stylelint": "^10.1.0",
    "umi": "^2.8.7",
    "umi-plugin-ga": "^1.1.3",
    "umi-plugin-pro": "^1.0.2",
    "umi-plugin-pro-block": "^1.3.2",
    "umi-plugin-react": "^1.9.5",
    "umi-request": "^1.0.8",
    "umi-types": "^0.3.8",
    "webpack-theme-color-replacer": "^1.2.15"
  }
}
